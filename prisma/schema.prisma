generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./wallet.sqlite"
}

model Wallet {
  id             String   @id @default(uuid())
  seed           String
  mnemonic       String
  representative String?
  created_at     DateTime @default(now())
  updated_at     DateTime @updatedAt
}
